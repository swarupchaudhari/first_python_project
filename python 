def clean_row(row):
    return row.strip().title()


import csv

with open('data.csv', 'r') as file:
    reader = csv.reader(file)
    for row in reader:
        print(row)

import pandas as pd

df = pd.read_excel("data.xlsx")
df.drop_duplicates(inplace=True)
df.fillna("NA", inplace=True)
df.to_excel("cleaned_data.xlsx", index=False)